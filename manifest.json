{
 "name": "Come on DuckDuckGo!",
 "description": "An extension which makes your default search DuckDuckGo and redirects you to duckduckgo.com if you go to Google, Bing, Yahoo, Yandex, Baidu or Sogou",
 "version": "1.6",
 "manifest_version": 2,
 "icons": {"48": "Come_on_DuckDuckgo_48.png"},
 "content_scripts": [
 {
    "matches": [
        "https://www.google.com/*",
        "https://www.google.ca/*",
        "https://www.google.com.hk/*",
        "https://www.google.com.au/*",
        "https://www.google.co.in/*",
        "https://www.google.co.jp/*",
        "https://www.google.com.ar/*",
        "https://www.google.com.vn/*",
        "https://www.google.com.mx/*",
        "https://www.google.de/*",
        "https://www.google.fr/*",
        "https://www.google.hu/*",
        "https://www.google.it/*",
        "https://www.google.nl/*",
        "https://www.google.pl/*",
        "https://www.google.pt/*",
        "https://www.bing.com/*",
        "https://www.yahoo.com/*",
        "https://www.yahoo.co.jp/*",
        "https://yandex.ru/*",
        "https://yandex.com/*",
        "https://www.aol.com/*",
        "https://www.aol.co.uk/*",
        "https://www.ask.com/*",
        "https://www.egerin.com/*",
        "https://startpage.com/*",
        "https://www.webcrawler.com/*",
        "https://yippy.com/*",
        "https://www.swisscows.com/*",
        "https://www.ecosia.org/*",
        "https://www.baidu.com/*",
        "https://www.sogou.com/*"
    ],
    "js": ["redirect.js"]
 }
 ],
 "chrome_settings_overrides": {
       "search_provider": {
         "favicon_url": "https://duckduckgo.com/favicon.ico",
         "encoding": "UTF-8",
         "is_default": true,
         "keyword": "duckduckgo.com",
         "name": "DuckDuckGo",
         "search_url": "https://duckduckgo.com/?q={searchTerms}&t=newext",
         "suggest_url": "https://duckduckgo.com/ac/?q={searchTerms}&type=list"
       }
       },
 "browser_action": {
     "default_icon": "Come_on_DuckDuckgo.png",
     "default_popup": "extension.html"
 }
}
